{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug test-mms",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/bin/test-mms",
        "args": [],
        "cwd": "${workspaceFolder}/bin",
        "preLaunchTask": "CMake: build tests",
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  